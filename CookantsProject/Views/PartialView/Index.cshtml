@model CookantsEntity.Model.AboutUs
<div class="container">
    <div class="row">
        <div class="col-md-6">
            <img class="img-responsive" src="@Html.DisplayFor(model => model.RootUrl)" alt="About">
        </div>
        <div class="col-md-6">
            <h1>@Html.DisplayFor(model => model.Title)</h1>
            @Html.Encode(@Html.DisplayFor(model => model.ShortDescription))
        </div>
    </div>
</div>


