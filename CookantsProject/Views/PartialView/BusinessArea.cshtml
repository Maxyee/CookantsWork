@model IEnumerable<CookantsEntity.Model.BusinessArea>
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h1 class="section-title">Order by Area</h1>
            <ul class="area-list">
                @foreach (var item in Model)
                {
                <li><a href="#">@Html.DisplayFor(modelItem => item.Name)</a></li>   
                }
            </ul>
        </div>
    </div>
</div>


